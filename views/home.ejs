<html>
	<head>
		<title>Latin Or Germanic?</title>
		<link rel="stylesheet" type="text/css" href="styles/style.css" />
	</head>
	<body>
		<h1>Latin Or Germanic?</h1>

		<h2 class="word"><% if (word) { %> <%= word %> <% } %></h2>

		<button class="guess" id="latin">LATIN
		</button>

		<button class="guess" id="germanic">GERMANIC
		</button>

		<h3 class="end-msg success-msg">Success!</h3>
		<h3 class="end-msg fail-msg">WRONG!</h3>

		<% if (etymology) { %> <p class="etymology"><%= etymology %></p> <% } %></p>

		<button id="new_game">New game</button>

		<script type="text/javascript" src="scripts/main.js"></script>
	</body>
</html>
